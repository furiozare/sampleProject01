{% set currentPath = app.request.attributes.get('_route') %}
{% set currentUrl = path(app.request.attributes.get('_route'),app.request.attributes.get('_route_params')) %}
<!DOCTYPE html>
<html lang="en" ng-app="app">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="apple-touch-icon-precomposed" sizes="144x144"
          href="{{ asset('assets/ico/apple-touch-icon-144-precomposed.png') }}">
    <link rel="apple-touch-icon-precomposed" sizes="114x114"
          href="{{ asset('assets/ico/apple-touch-icon-114-precomposed.png') }}">
    <link rel="apple-touch-icon-precomposed" sizes="72x72"
          href="{{ asset('assets/ico/apple-touch-icon-72-precomposed.png') }}">
    <link rel="apple-touch-icon-precomposed" href="{{ asset('assets/ico/apple-touch-icon-57-precomposed.png') }}">
    <link rel="shortcut icon" href="{{ asset('assets/ico/favicon.png') }}">

    <title>{% block tittle %}Bahana Yamaha{% endblock %}</title>
    {% block defaultStylesheet %}
        <link href="{{ asset('bower_components/bootstrap/dist/css/bootstrap.css') }}" rel="stylesheet">
        <link href="{{ asset('bower_components/font-awesome/css/font-awesome.css') }}" rel="stylesheet">
        <link href="{{ asset('assets/css/styles.css') }}" rel="stylesheet">
        <link href="{{ asset('assets/css/blog.css') }}" rel="stylesheet">

        <link href="{{ asset('bower_components/ng-ckeditor/ng-ckeditor.css') }}" rel="stylesheet">
        <link rel='stylesheet' href='{{ asset('bower_components/angular-loading-bar/build/loading-bar.min.css') }}'
              type='text/css' media='all'/>
        <link rel="stylesheet" type="text/css"
              href="{{ asset('bower_components/angular-busy/dist/angular-busy.min.css') }}"/>
        <link href="{{ asset('bower_components/angular-aside/dist/css/angular-aside.min.css') }}" rel="stylesheet">
        <link href="{{ asset('bower_components/ng-directive-lazy-image/release/lazy-image-style.min.css') }}"
              rel="stylesheet">
        <link href="{{ asset('bower_components/angucomplete-alt/angucomplete-alt.css') }}" rel="stylesheet">
    {% endblock %}
    {% block stylesheet %}{% endblock %}
    <script src="http://maps.googleapis.com/maps/api/js"></script>
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    {% block defaultJavascript %}
        <script type="text/javascript">
            var base_url = "{{ asset('', absolute=true) }}";
            var current_url = "{{ path(app.request.attributes.get('_route'),app.request.attributes.get('_route_params')) }}";
            var default_password = "{{ default_password }}";
        </script>
        <script src="{{ asset('bower_components/jquery/dist/jquery.min.js') }}"></script>
        <script src="{{ asset('bower_components/bootstrap/dist/js/bootstrap.min.js') }}"></script>

        <script src="{{ asset('bower_components/angular/angular.min.js') }}"></script>
        <script src="{{ asset('bower_components/angular-i18n/angular-locale_id-id.js') }}"></script>
        <script src="{{ asset('bower_components/angular-animate/angular-animate.min.js') }}"></script>
        <script src="{{ asset('bower_components/angular-bootstrap/ui-bootstrap-tpls.min.js') }}"></script>
        <script src="{{ asset('bower_components/bootstrap-ui-datetime-picker/dist/datetime-picker.min.js') }}"></script>
        <script src="{{ asset('bower_components/moment/min/moment-with-locales.min.js') }}"></script>
        <script src="{{ asset('bower_components/angular-moment/angular-moment.min.js') }}"></script>
        <script src="{{ asset('bower_components/angular-busy/dist/angular-busy.min.js') }}"></script>
        <script src="{{ asset('bower_components/angular-loading-bar/build/loading-bar.min.js') }}"></script>
        <script src="{{ asset('bower_components/angular-file-model/angular-file-model.js') }}"></script>
        <script src="{{ asset('bower_components/ckeditor/ckeditor.js') }}"></script>
        <script src="{{ asset('bower_components/ng-ckeditor/ng-ckeditor.min.js') }}"></script>
        <script src="{{ asset('bower_components/angular-aside/dist/js/angular-aside.min.js') }}"></script>
        <script src="{{ asset('bower_components/angular-google-chart/ng-google-chart.min.js') }}"></script>
        <script src="{{ asset('bower_components/ng-directive-lazy-image/release/lazy-image.min.js') }}"></script>
        <script src="{{ asset('bower_components/angular-resource/angular-resource.min.js') }}"></script>
        <script src="{{ asset('bower_components/angucomplete-alt/dist/angucomplete-alt.min.js') }}"></script>

        <script src="{{ asset('angApp/app.js') }}"></script>
    {% endblock %}

    <script src="{{ asset('angApp/resource/front/emailSubscribe.js') }}"></script>
    <script src="{{ asset('angApp/controller/emailSubscribeController.js') }}"></script>

    {% block javascript %}{% endblock %}
</head>
<body>

{{ render(controller('AppBundle:Front/Template:header', {currentPath: currentPath, currentUrl: currentUrl})) }}

{% block content %}
{% endblock %}

{{ render(controller('AppBundle:Front/Template:footer', {currentPath: currentPath, currentUrl: currentUrl})) }}

{% block javascriptPlugins %}
    <script type="text/javascript" src="{{ asset('assets/js/idangerous.swiper-2.1.min.js') }}"></script>
    <script>
        var mySwiper = new Swiper('.swiper-container', {
            pagination: '.box-pagination',
            keyboardControl: true,
            paginationClickable: true,
            slidesPerView: 'auto',
            autoResize: true,
            resizeReInit: true
        });

        $('.prevControl').on('click', function (e) {
            e.preventDefault();
            mySwiper.swipePrev();
        });
        $('.nextControl').on('click', function (e) {
            e.preventDefault();
            mySwiper.swipeNext();
        })
    </script>

    <script src="{{ asset('assets/js/jquery.cycle2.min.js') }}"></script>
    <script src="{{ asset('assets/js/jquery.easing.1.3.js') }}"></script>
    <script type="text/javascript" src="{{ asset('assets/js/jquery.parallax-1.1.js') }}"></script>
    <script type="text/javascript" src="{{ asset('assets/js/helper-plugins/jquery.mousewheel.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('assets/js/jquery.mCustomScrollbar.js') }}"></script>
    <script type="text/javascript" src="{{ asset('assets/plugins/icheck-1.x/icheck.min.js') }}"></script>
    <script src="{{ asset('assets/js/grids.js') }}"></script>
    <script src="{{ asset('assets/js/owl.carousel.min.js') }}"></script>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/select2/4.0.0/js/select2.min.js"></script>
    <script src="{{ asset('assets/js/bootstrap.touchspin.js') }}"></script>
    <script src="{{ asset('assets/js/home.js') }}"></script>
    <script src="{{ asset('assets/js/script.js') }}"></script>
{% endblock %}
</body>
</html>